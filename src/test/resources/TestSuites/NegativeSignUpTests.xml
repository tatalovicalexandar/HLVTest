<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="NegativeSignUpTestSuite" verbose="1" parallel="none">

	<listeners>
		<listener class-name="local.HLVTestApp.listeners.TestListener"></listener>
	</listeners>
	
	<test name="NegativeSignUpTests-CH">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="local.HLVTestApp.loginandsignuptests.NegativeSignUpTests">
			</class>
		</classes>
	</test>
	
	<test name="NegativeSignUpTests-FF">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="local.HLVTestApp.loginandsignuptests.NegativeSignUpTests">
			</class>
		</classes>
	</test>

</suite>
