<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="PositiveLoginTestSuite" parallel="tests" verbose="1">
	
		<listeners>
			<listener class-name="local.HLVTestApp.listeners.TestListener"></listener>
		</listeners>
		
		<test name="PositiveLoginOnMainPageTest1-CH">
			<parameter name="browser" value="chrome"></parameter>
			<classes>
				<class name="local.HLVTestApp.loginandsignuptests.PositiveLoginTests">
				</class>
			</classes>
		</test>
		
		<test name="PositiveLoginOnMainPageTest2-FF">
			<parameter name="browser" value="firefox"></parameter>
			<classes>
				<class name="local.HLVTestApp.loginandsignuptests.PositiveLoginTests">
				</class>
			</classes>
		</test>
	
</suite>
